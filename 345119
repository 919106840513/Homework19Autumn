using namespace std;
#include <iostream>
int main()
{
	int a,b;
	cin>>a>>b;
	if(a>b)
	{
		for(int i=b;i>=1;i--)
		if(a%i==0&&b%i==0)
		{
			cout<<a<<","<<b<<"的最大公约数是"<<i<<endl;
			break; 
		}
		for(int t=a;t<=a*b;t++)
		if(t%a==0&&t%b==0)
		{
			cout<<a<<","<<b<<"的最大公倍数是"<<t<<endl;
			break;
		}
		
    }
		if(b>a)
	{
		for(int i=a;i>=1;i--)
		if(a%i==0&&b%i==0)
		{
			cout<<a<<","<<b<<"的最大公约数是"<<i<<endl;
			break; 
		}
		for(int t=b;t<=a*b;t++)
		if(t%a==0&&t%b==0)
		{
			cout<<a<<","<<b<<"的最大公倍数是"<<t<<endl;
			break;
		}
	}
		if(a==b){
		cout<<a<<","<<b<<"的最大公约数是"<<a<<endl;
		cout<<a<<","<<b<<"的最大公倍数是"<<a<<endl;
             	}
		return 0;
		
}
