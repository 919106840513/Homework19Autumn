(2)
#include<iostream>
#include<stdio.h>
#include<stdlib.h>
using namespace std;
int main(void){
	int n;
	int a[n];
	cin>>n;
	for(int i=0;i<n;i++){
		a[i]=rand()%100;
	}
	int i,j;
	for(i=0;i<n-1;i++){
		j=i+1;
		for(;a[i]>=a[j],j<n;j++){
		}
		if(j=n-1){
			cout<<i<<endl;
			break;
			}
	}
	return 0;


}






(5)
#include<iostream>
using namespace std;
int main(){
	int n,m,i,k,t;
	cin>>n;
    k=0,t=0,m=2;
	i=n;
	for(;m<n;m++){
		if(n%m==0){
			n=n/m;
			for(;m>=10;m=m/10){
				t=m%10;
				k=k+t;
			}		
			if(m<10){
				k=k+m; 
			}
		}
		m=1;
	}
	for(;n>=10;n=n/10){
		t=n%10;
		k=k+t;
	}
	k=k+n;
	if(k==i){
		cout<<i<<endl;
	}
		return 0;
	}
  
  
  
  (10)#include<iostream>
using namespace std;
int main(){
	int a[21];
	int i,k,j;
	a[i]=i;
	for(k=0,j=1;k<20;j++){
		if(a[j]%5==0){
			a[j]=a[j-1]+0;
			k++;
		}
		else{a[j+1]=a[j]+1;
				}
	}
	j=j%21;
	cout<<j<<endl;
	return 0;
	
}
  
